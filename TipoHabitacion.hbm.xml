<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name= "datos.TipoHabitacion" table="TipoHabitacion">
<id column="idTipoHabitacion" name="idTipoHabitacion" >
<generator class="identity"></generator>
</id>
<property name="descripcion" column="descripcion" type="string"/>
<property name="precioTemporadaBaja" column="precioTemporadaBaja" type="float"/>
<property name="precioTemporadaAlta" column="precioTemporadaAlta" type="float"/>

<one-to-one name="Habitacion" class="datos.Habitacion"></one-to-one>
</class>
</hibernate-mapping>
